! special
Xft.dpi: 162
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! another dpi setting, the last one read will be used
xft.dpi: 120

! colors
*.foreground: #C6C6C6
*.background: #1C1C1C
*.cursorColor: #C6C6C6

! black
*.color0: #303030
*.color8: #444444
! trueblack
*.trueblack: #000000

! white
*.color7: #4E4E4E
*.color15: #ffffff

