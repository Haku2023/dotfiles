{
  "description": "Main(Firefox),Sub(Chrome,MRD) Command :  j → click on page ; 2. Option Enter → Command Enter",
  "manipulators": [
    {
      "conditions": [
        {
          "bundle_identifiers": ["^org.mozilla.firefox$"],
          "type": "frontmost_application_if"
        }
      ],
      "from": {
        "key_code": "j",
        "modifiers": {
          "mandatory": ["option"],
          "optional": []
        }
      },
      "to": [
        {
          "key_code": "5",
          "modifiers": ["command", "control", "shift"]
        }
      ],
      "type": "basic"
    },
    {
      "conditions": [
        {
          "bundle_identifiers": ["^com.google.Chrome$"],
          "type": "frontmost_application_if"
        }
      ],
      "from": {
        "key_code": "j",
        "modifiers": {
          "mandatory": ["option"],
          "optional": []
        }
      },
      "to": [
        {
          "key_code": "4",
          "modifiers": ["command", "control", "shift"]
        }
      ],
      "type": "basic"
    },
    {
      "conditions": [
        {
          "bundle_identifiers": [
            "^com.google.Chrome$",
            "^org.mozilla.firefox$",
            "^org.qutebrowser.qutebrowser$"
          ],
          "type": "frontmost_application_if"
        }
      ],
      "from": {
        "key_code": "return_or_enter",
        "modifiers": {
          "mandatory": ["option"],
          "optional": []
        }
      },
      "to": [
        {
          "key_code": "return_or_enter",
          "modifiers": ["command"]
        }
      ],
      "type": "basic"
    }
  ]
}
